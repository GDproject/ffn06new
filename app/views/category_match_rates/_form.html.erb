<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @load_category_rates do |f| %>
      <%= render "shared/error_messages", object: @load_category_rates %>
      <%= f.label :category_rates, class: "col-md-4 control-label" %>
      <%= f.text_field :category_rates, class: "form-control" %><br>

      <%= f.submit %>
    <% end %>
  </div>
</div>
