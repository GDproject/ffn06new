<div class="row" style="margin-bottom:20px; margin-top:20px; float:right;">
  <%= link_to t("category_rates.new"), new_admin_category_match_rate_path, class: "btn btn-sm btn-primary btn-create" %>
</div>
<div class="row">
    <table class="table table-striped table-bordered table-list">
      <thead>
        <tr>
            <th class="hidden-xs">CATALOGY MATCH RATES</th>
        </tr>
      </thead>
      <tbody>
      <% @categories.each do |category| %>
              <tr>
                <td><%= link_to category.category_rates, admin_category_match_rate_path(category) %></td>
              </tr>
               <% end %>
            </tbody>
    </table>
  <%= will_paginate @categories %>
</div>
