<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for [:admin, @category_news] do |f| %>
      <%= render "shared/error_messages", object: @category_news %>

      <%= f.label :category_name, class: "col-md-4 control-label" %>
      <%= f.text_field :category_name, class: "form-control" %><br>

      <%= f.submit %>
    <% end %>
  </div>
</div>
